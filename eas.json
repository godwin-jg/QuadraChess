{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON"
      },
      "prebuildCommand": "node create-google-services.js"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON"
      },
      "prebuildCommand": "node create-google-services.js"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON"
      },
      "prebuildCommand": "node create-google-services.js"
    },
    "production-small": {
      "autoIncrement": true,
      "env": {
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "prebuildCommand": "node create-google-services.js"
    }
  },
  "submit": {
    "production": {}
  }
}
